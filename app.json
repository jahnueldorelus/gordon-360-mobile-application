{
  "expo": {
    "name": "360 DM",
    "owner": "gordon_college",
    "slug": "360_DM",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "edu.Gordon360.DM",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "edu.Gordon360.DM"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
